{
  "name": "@oomol-lab/ovm",
  "version": "2.0.0-rc.11",
  "description": "oomol virtual machine",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "out/index.js",
    "module": "out/index.mjs",
    "types": "out/index.d.ts"
  },
  "ovm": {
    "darwin": {
      "core": "v1.0.1",
      "ovm": "v2.0.0-RC4"
    },
    "win32": {
      "core": "v1.0.0-RC2",
      "ovm": "v1.0.0-RC2"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./out/index.d.mts",
        "default": "./out/index.mjs"
      },
      "require": {
        "types": "./out/index.d.ts",
        "default": "./out/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup-node",
    "lint": "eslint .",
    "ts-check": "tsc --noEmit",
    "postinstall": "node ./scripts/postinstall.mjs"
  },
  "keywords": [
    "oomol",
    "vm",
    "ovm"
  ],
  "author": "BlackHole1 <bh@bugs.cc>",
  "license": "MIT",
  "files": [
    "out",
    "src",
    "scripts"
  ],
  "devDependencies": {
    "@oomol-lab/eslint-config-basic": "^0.0.1",
    "@oomol-lab/eslint-config-ts": "^0.0.2",
    "@oomol-lab/tsconfig": "^0.0.1",
    "@types/node": "^20.6.5",
    "got": "^14.4.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "remitter": "^0.4.1"
  }
}
