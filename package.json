{
  "name": "@oomol-lab/ovm",
  "version": "0.0.1",
  "description": "oomol virtual machine",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "out/index.js",
    "module": "out/index.mjs",
    "types": "out/index.d.ts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./out/index.d.mts",
        "default": "./out/index.mjs"
      },
      "require": {
        "types": "./out/index.d.ts",
        "default": "./out/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup-node",
    "lint": "eslint .",
    "ts-check": "tsc --noEmit"
  },
  "keywords": [
    "oomol",
    "vm",
    "container"
  ],
  "author": "BlackHole1 <bh@bugs.cc>",
  "license": "MIT",
  "files": [
    "out",
    "src"
  ],
  "devDependencies": {
    "@oomol-lab/eslint-config-basic": "^0.0.1",
    "@oomol-lab/eslint-config-ts": "^0.0.2",
    "@oomol-lab/tsconfig": "^0.0.1",
    "@types/node": "^20.6.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "node-ssh": "^13.1.0",
    "remitter": "^0.3.2"
  }
}
